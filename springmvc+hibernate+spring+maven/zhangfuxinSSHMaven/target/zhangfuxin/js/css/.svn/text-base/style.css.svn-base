/*单选按钮、复选按钮*/
.custom-checkbox{ position: relative; }
/* input, label positioning */
.custom-checkbox input{width:13px;height:13px;position: absolute;left: 2px;top: 0px;margin: 0;z-index: 0;}
.custom-checkbox label{ width:80px; height:13px; display: block; position: relative; z-index: 1; line-height:1; padding-left:20px; cursor:pointer; float:left; font-family:"微软雅黑"; font-size:12px; color: #fff;}
.forget{width:110px;height:16px;line-height:12px; text-align:right; float:right;}
.forget a{height:16px;line-height:12px;font-family:"微软雅黑"; font-size:12px; color: #fff;}
.forget a:hover{color:#3faceb;}
/* ==默认状态效果== */
.custom-checkbox label { background:url(../images/divide.png) no-repeat; }
.custom-checkbox label{background-position: 2px -15px;}
/*==鼠标悬停和得到焦点状态==*/
.custom-checkbox label.hover,
.custom-checkbox label.focus{background-position: 2px -30px;}
/*==选中状态==*/
.custom-checkbox label.checked{background-position: 2px 0px;}
.custom-checkbox label.checkedHover,
.custom-checkbox label.checkedFocus {background-position: 2px 0px;}
/*单选按钮、复选按钮结束*/

