.custom-radio { width:120px; margin-top:7px; margin-left:4px; float:left; position: relative; }
.custom-radio input {width:13px;height:13px;position: absolute;left: 2px;top: 0px;margin: 0;z-index: 0;}
.custom-radio label {height:13px; display: block;position: relative;z-index: 1;line-height: 1; padding-left:20px;cursor: pointer;font-family:"微软雅黑"; font-size:12px; color:#888;}
/* ==默认状态效果== */
.custom-radio label { background: url(../images/divide-1.png) no-repeat; }
.custom-radio label {background-position: 2px -15px;}
/*==鼠标悬停和得到焦点状态==*/
.custom-radio label.hover,
.custom-radio label.focus {background-position: 2px -30px;}
/*==选中状态==*/
.custom-radio label.checked {background-position: 2px 0px;}
.custom-checkbox label.checkedFocus {background-position: 2px 0px;}
/*单选按钮、复选按钮结束*/

